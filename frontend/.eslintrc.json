{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "quotes": "off",
    "no-console": "off",
    "no-debugger": "warn",
    "no-unused-vars": "warn",
    "no-undef": "error",
    "no-unreachable": "error",
    "eqeqeq": "warn",
    "curly": "off",
    "no-extra-semi": "warn"
  }
}
